{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "outDir": "./js",
    "rootDir": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["src/**/*", "tsup.config.ts"],
  "exclude": ["node_modules"]
}
